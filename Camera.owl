<!-- Defining Properties in OWL -->
<!-- OWL uses two types of Properties called owl:ObjectProperty and owl:DatatypeProperty -->
<!-- owl:ObjectProperty is used to link resources to resources -->
<!-- owl:DatatypeProperty is used to link resources to rdfs:Literal (untyped) or an XML schema built-in datatype (typed) value -->
<!-- Both owl:ObjectProperty and owl:DatatypeProperty are sub-properties of rdf:Property -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">  

    <!-- #### Classes #### -->

    <!-- owned_by in RDFS -->
    <!--
    <rdfs:Property rdf:ID="owned_by">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Photographer"/>
    </rdfs:Property>
    -->

    <!-- owned_by in OWL -->
    <owl:ObjectProperty rdf:ID="owned_by">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Photographer"/>
    </owl:ObjectProperty>

    <!-- expensiveOrNot in RDFS -->
    <!---
    <rdfs:Property rdf:ID="expensiveOrNot">
        <rdfs:domain rdf:resource="#Digital"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </rdfs:Property>
    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
    -->

    <!-- expensiveOrNot in OWL -->
    <owl:DatatypeProperty rdf:ID="expensiveOrNot">
        <rdfs:domain rdf:resource="#Digital"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#string"/>


</rdf:RDF>



<!-- Updated OWL including Properties -->
<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">  

    <!-- #### Classes #### -->
    <owl:Class rdf:ID="Camera">
    </owl:Class>

    <owl:Class rdf:ID="Person">
    </owl:Class>

    <owl:Class rdf:ID="Specifications">
    </owl:Class>

    <owl:Class rdf:ID="Digital">
        <rdfs:subClassOf rdf:resource="#Camera"/>
    </owl:Class>

    <owl:Class rdf:ID="Film">
        <rdfs:subClassOf rdf:resource="#Camera"/>
    </owl:Class>

    <owl:Class rdf:ID="SLR">
        <rdfs:subClassOf rdf:resource="#Digital"/>
    </owl:Class>

    <owl:Class rdf:ID="PointAndShoot">
        <rdfs:subClassOf rdf:resource="#Digital"/>
    </owl:Class>

    <owl:Class rdf:ID="Photographer">
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>

    <owl:Class rdf:ID="Professional">
        <rdfs:subClassOf rdf:resource="#Photographer"/>
    </owl:Class>

    <owl:Class rdf:ID="Amateur">
        <rdfs:subClassOf rdf:resource="#Photographer"/>
    </owl:Class>

    <owl:Class rdf:ID="ExpensiveSLR">
        <rdfs:subClassOf rdf:resource="#SLR"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#owned_by"/>
                <owl:someValuesFrom rdf:resource="#Professional"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <!-- #### Properties #### -->

    <owl:ObjectProperty rdf:ID="owned_by">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Photographer"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:ID="has_spec">
        <rdfs:domain rdf:resource="#SLR"/>
        <rdfs:range rdf:resource="#Specifications"/>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:ID="expensiveOrNot">
        <rdfs:domain rdf:resource="#Digital"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    <rdfs:Datatype rdf:about="&xsd;string"/>

    <owl:DatatypeProperty rdf:ID="model">
        <rdfs:domain rdf:resource="#Specifications"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    <rdfs:Datatype rdf:about="&xsd;string"/>

    <owl:DatatypeProperty rdf:ID="pixel">
        <rdfs:domain rdf:resource="#Digital"/>
        <rdfs:range rdf:resource="http://www.someStandard.org/MegaPixel"/>
    </owl:DatatypeProperty>
    <rdfs:Datatype rdf:about="http://www.someStandard.org/MegaPixel">
        <rdfs:subClassOf rdf:resource="&xsd;decimal"/>
    </rdfs:Datatype>

</rdf:RDF>



<!-- OWL Property Features -->
<!-- Properties can be Symmetric, Transitive, Functional, InverseFunctional, Inverse of another property -->
<!-- Note: Uses rdf:type not rdfs -->


<!-- OWL Property Features - Symmetric Properties -->
<!-- If resource R1 is connected to R2 by a property, then R2 is connected to R1 by the same property -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">

    <!-- #### Classes #### -->

    <!-- #### Properties #### -->

    <owl:ObjectProperty rdf:ID="friend_with">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#Person"/>
        <!-- We just use a rdf:type to specify the property type -->
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    </owl:ObjectProperty>

    <!-- The respective RDF representation will be like -->
    <!--
    <Photographer rdf:about="http://www.yuchen.net/people#Liyang">
        <friend_with rdf:resource="http://www.yuchen.net/people#Jin"/>
    </Photographer>
    -->
    <!-- Since Liyang is friend with Jin, Jin is also friend with Liyang -->

</rdf:RDF>



<!-- OWL Property Features - Transitive Properties -->
<!-- If resource R1 is connected to R2 by a property, and R2 is connected to R3 by the same property, then R1 is connected to R3 by the same property -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">

    <!-- #### Classes #### -->

    <!-- #### Properties #### -->

    <!-- Define a new property called betterQualityPriceRatio as a transitive property, so that can be used to compare cameras  -->
    <owl:ObjectProperty rdf:ID="betterQualityPriceRatio">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Camera"/>
        <!-- We just use a rdf:type to specify the property type -->
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>

    <!-- If Camera A has betterQualityPriceRatio with Camera B, and Camera B has betterQualityPriceRatio with Camera C, then Camera A has betterQualityPriceRatio with Camera C -->

</rdf:RDF>



<!-- OWL Property Features - Functional Properties -->
<!-- A property is functional if for a given resource, there can be atmost one value for that property -->
<!-- Many-To-One - atmost one unique value for each instance -->
<!-- For a given rdfs:domain, there is only one rdfs:model value -->
<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">

    <!-- #### Classes #### -->

    <!-- #### Properties #### -->

    <owl:DatatypeProperty rdf:ID="model">
        <rdfs:domain rdf:resource="#Specifications"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    <rdfs:Datatype rdf:about="&xsd;string"/>

    <!-- In main rdf document the object is created like this -->
    <!--
    <Specifications rdf:about="http://www.yuchen.net/photography/NikonD70.rdf#Specifications">
        <model rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
            Nikon D70
        </model>
    </Specifications>
    -->

    <!-- Another person mentions this object URI in a different document -->
    <!-- Note that it uses the same URI and also uses the same othology in the default namespace  -->
    <!--
    <Specifications rdf:about="http://www.yuchen.net/photography/NikonD70.rdf#Specifications" xmlns="http://www.yuchen.net/photography/Camera.owl">
        <model rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
            Nikon-D70
        </model>
    </Specifications>
    -->

    <!-- Because model is a functional property, both values should be the same. So, Nikon D70 and Nikon-D70 should be considered the same. -->

</rdf:RDF>



<!-- OWL Property Features - Inverse Properties -->
<!-- If resource R1 is connected to R2 by a property P, then R2 is connected to R1 by the inverse of property P -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">

    <!-- #### Classes #### -->

    <!-- #### Properties #### -->

    <owl:ObjectProperty rdf:ID="owned_by">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Photographer"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:ID="owns">
        <!-- owl:inverseOf is used to specify that owns is the inverse property of owned_by -->
        <owl:inverseOf rdf:resource="#owned_by"/>
        <rdfs:domain rdf:resource="#Photographer"/>
        <rdfs:range rdf:resource="#Camera"/>
    </owl:ObjectProperty>

</rdf:RDF>



<!-- OWL Property Features - Inverse Functional Properties -->
<!-- A property is inverse functional if for a given value, there can be atmost one resource that has that value for that property -->
<!-- One-To-Many - atmost one unique subject for each value -->
<!-- For a given rdfs:model, there is only one rdfs:domain value -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">

    <!-- #### Classes #### -->

    <!-- #### Properties #### -->

    <owl:ObjectProperty rdf:ID="owned_by">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Photographer"/>
    </owl:ObjectProperty>

    <!-- We can define as for a given Camera there is a unique Photographer who owns it -->
    <owl:ObjectProperty rdf:ID="owns">
        <!-- We just use a rdf:type to specify the property type -->
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="#Photographer"/>
        <rdfs:range rdf:resource="#Camera"/>
    </owl:ObjectProperty>

    <!-- In main rdf document the object is created like this -->
    <!--
    <Photographer rdf:ID="LiyangJin">
        <owns rdf:resource="http://www.yuchen.net/photography/NikonD70.rdf#Camera"/>
    </Photographer>
    -->

    <!-- Another person mentions this object URI in a different document -->
    <!-- Note that it uses the same URI and also uses the same othology in the default namespace  -->
    <!--
    <Photographer rdf:ID="JinLiyang" xmlns="http://www.yuchen.net/photography/Camera.owl">
        <owns rdf:resource="http://www.yuchen.net/photography/NikonD70.rdf#Camera"/>
    </Photographer>
    -->

    <!-- Because owns is an inverse functional property, both subjects should be the same. So, LiyangJin and JinLiyang should be considered the same. -->

</rdf:RDF>



<!-- OWL Ontology Matching Features - owl:equivalentClass -->
<!-- Used to explicilty declare that two classes in two different onthologies are equivalent classes -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">

    <!-- #### Classes #### -->

    <owl:Class rdf:ID="Digital">
        <rdfs:subClassOf rdf:resource="#Camera"/>
        <!-- owl:equivalentClass is used to specify that this Digital class is equivalent to DigitalCamera class defined in a different ontology -->
        <owl:equivalentClass rdf:resource="http://www.anotherOntology.org/ontologies/Photography.owl#DigitalCamera"/>
    </owl:Class>

    <owl:Class rdf:ID="SLR">
        <rdfs:subClassOf rdf:resource="#Digital"/>
        <!-- owl:equivalentClass is used to specify that this SLR class is equivalent to SingleLensReflex class defined in a different ontology -->
        <owl:equivalentClass rdf:resource="http://www.anotherOntology.org/ontologies/Photography.owl#SingleLensReflex"/>
    </owl:Class>

    <!-- #### Properties #### -->

</rdf:RDF>



<!-- OWL Ontology Matching Features - owl:disjointWith -->
<!-- Used to specify that two classes are not related in any way -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">

    <!-- #### Classes #### -->

    <owl:Class rdf:ID="SLR">
        <rdfs:subClassOf rdf:resource="#Digital"/>
        <!-- owl:disjointWith is used to specify that SLR class is disjoint with PointAndShoot class -->
        <!-- owl:disjointWith is by default a symmetric property. So PointAndShoot is also disjoint with SLR. -->
        <owl:disjointWith rdf:resource="#PointAndShoot"/>
    </owl:Class>
    
    <!-- #### Properties #### -->

</rdf:RDF>



<!-- owl:AllDifferent - If we have more instances that are different from each other  -->
<?xml version="1.0" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://www.yuchen.net/photography/Camera.owl#">  

    <!-- #### Classes #### -->

    <!-- owl:AllDifferent is used to state that all the instances listed are different from each other -->
    <owl:AllDifferent>
        <!-- owl:distinctMembers indicates that the following instances are all different from each other -->
        <!-- rdf:parseType="Collection" means that the enclosed elements are treated as a collection -->
        <owl:distinctMembers rdf:parseType="Collection">
            <SLR rdf:about="http://www.yuchen.net/rdf/NikonD70.rdf#NikonD70"/>
            <SLR rdf:about="http://www.yufriend.net/rdf/NikonD70.rdf#AsianNikonD70"/>
            <SLR rdf:about="http://www.anotherfriend.net/rdf/NikonD70.rdf#EuropeanNikonD70"/>
        </owl:distinctMembers>
    </owl:AllDifferent>  

    <!-- #### Properties #### -->
    
</rdf:RDF>



<!-- Ontology Header -->
<!-- This header includes rdfs:comment, rdfs:label, owl:priorVersion, owl:versionInfo for Humans -->
<!-- And also owl:imports for Machines -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">

    <!-- Notice that owl:Ontology has a rdf:about attribute to specify the URI of the ontology -->
    <owl:Ontology rdf:about="http://www.yuchen.net/photography/Camera.owl">
        <!-- rdfs:label is used to provide a human-readable name for the ontology -->
        <rdfs:label>
            Camera Ontology
        </rdfs:label>
        <!-- rdfs:comment is used to provide a human-readable description of the ontology -->
        <rdfs:comment>
            This ontology describes different types of cameras, photographers, and their relationships.
        </rdfs:comment>
        <!-- owl:versionInfo is used to specify the version information of this ontology -->
        <owl:versionInfo>
            Version 1.0 - Initial release
        </owl:versionInfo>
        <!-- owl:priorVersion is used to specify the previous version of this ontology -->
        <owl:priorVersion rdf:resource="http://www.yuchen.net/photography/Camera0.owl"/>
        <!-- owl:imports is used to specify that this ontology imports another ontology -->
        <owl:imports rdf:resource="http://www.anotherOntology.org/ontologies/Photography.owl"/>
    </owl:Ontology>

    <!-- #### Classes #### -->

    <!-- #### Properties #### -->


</rdf:RDF>



<!-- Full Camera Ontology Rewritten -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">


    <!-- #### Classes #### -->
    <owl:Class rdf:ID="Camera">
    </owl:Class>

    <owl:Class rdf:ID="Person">
    </owl:Class>

    <owl:Class rdf:ID="Specifications">
    </owl:Class>

    <owl:Class rdf:ID="Digital">
        <rdfs:subClassOf rdf:resource="#Camera"/>
        <owl:equivalentClass rdf:resource="http://www.anotherOntology.org/ontologies/Photography.owl#DigitalCamera"/>
    </owl:Class>

    <owl:Class rdf:ID="Film">
        <rdfs:subClassOf rdf:resource="#Camera"/>
    </owl:Class>

    <owl:Class rdf:ID="SLR">
        <rdfs:subClassOf rdf:resource="#Digital"/>
        <owl:disjointWith rdf:resource="#PointAndShoot"/>
        <owl:equivalentClass rdf:resource="http://www.anotherOntology.org/ontologies/Photography.owl#SingleLensReflex"/>
    </owl:Class>

    <owl:Class rdf:ID="PointAndShoot">
        <rdfs:subClassOf rdf:resource="#Digital"/>
    </owl:Class>

    <owl:Class rdf:ID="Photographer">
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>

    <owl:Class rdf:ID="Professional">
        <rdfs:subClassOf rdf:resource="#Photographer"/>
        <owl:disjointWith rdf:resource="#Amateur"/>
    </owl:Class>

    <owl:Class rdf:ID="Amateur">
        <rdfs:subClassOf rdf:resource="#Photographer"/>
    </owl:Class>

    <owl:Class rdf:ID="ExpensiveSLR">
        <rdfs:subClassOf rdf:resource="#SLR"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#owned_by"/>
                <owl:someValuesFrom rdf:resource="#Professional"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#expensiveOrNot"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    expensive
                </owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:AllDifferent>
        <owl:distinctMembers rdf:parseType="Collection">
            <SLR rdf:about="http://www.yuchen.net/rdf/NikonD70.rdf#NikonD70"/>
            <SLR rdf:about="http://www.yufriend.net/rdf/NikonD70.rdf#AsianNikonD70"/>
            <SLR rdf:about="http://www.anotherfriend.net/rdf/NikonD70.rdf#EuropeanNikonD70"/>
        </owl:distinctMembers>
    </owl:AllDifferent> 

    <!-- #### Properties #### -->

    <owl:ObjectProperty rdf:ID="owned_by">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Photographer"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:ID="has_spec">
        <rdfs:domain rdf:resource="#SLR"/>
        <rdfs:range rdf:resource="#Specifications"/>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:ID="expensiveOrNot">
        <rdfs:domain rdf:resource="#Digital"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#string"/>

    <owl:DatatypeProperty rdf:ID="model">
        <rdfs:domain rdf:resource="#Specifications"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#string"/>

    <owl:DatatypeProperty rdf:ID="pixel">
        <rdfs:domain rdf:resource="#Digital"/>
        <rdfs:range rdf:resource="http://www.someStandard.org/MegaPixel"/>
    </owl:DatatypeProperty>
    <rdfs:Datatype rdf:about="http://www.someStandard.org/MegaPixel">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </rdfs:Datatype>

    <owl:ObjectProperty rdf:ID="friend_with">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#Person"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:ID="betterQualityPriceRatio">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Camera"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:ID="owns">
        <owl:inverseOf rdf:resource="#owned_by"/>
        <rdfs:domain rdf:resource="#Photographer"/>
        <rdfs:range rdf:resource="#Camera"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    </owl:ObjectProperty>

</rdf:RDF>



